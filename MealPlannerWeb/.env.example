# Meal Planner App Environment Variables
# Copy this file to .env and fill in your values

# Firebase Configuration
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your-project-id
REACT_APP_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
REACT_APP_FIREBASE_APP_ID=your_app_id

# Firebase Functions URL
# For local development: http://localhost:5001/your-project-id/us-central1
# For production: https://us-central1-your-project-id.cloudfunctions.net
REACT_APP_FUNCTIONS_URL=http://localhost:5001/your-project-id/us-central1

# Environment
REACT_APP_ENVIRONMENT=development

# API Keys (Server-side only - Set in Firebase Functions config)
# firebase functions:config:set openai.api_key="your_key"
# firebase functions:config:set spoonacular.api_key="your_key" 